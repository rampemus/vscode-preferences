{
  "git.addedSign.hlGroup": "GitGutterAdd",
  "git.addedSign.text": "▎",
  "git.changedSign.hlGroup": "GitGutterChange",
  "git.changedSign.text": "▎",
  "git.modifiedSign.hlGroup": "GitGutterChange",
  "git.modifiedSign.text": "▎",
  "git.removedSign.hlGroup": "GitGutterDelete",
  "git.removedSign.text": "▁",
  "git.topRemovedSign.hlGroup": "GitGutterDelete",
  "git.topRemovedSign.text": "▔",
  "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
  "git.changeRemovedSign.text": "▎",
  "snippets.textmateSnippetsRoots": [
    "~/Library/Application Support/Code/User/snippets"
  ],
  "snippets.priority": 0,
  "explorer.keyMappings.global": {
    "o": ["wait", "expandable?", ["wait", "expanded?", "collapse", "expand"], "open"],
    "p": ["open", "normal:-"],
    "-": "collapse",
    "E": "open:split",
    "s": "open:vsplit",
		"d": "delete",
    "ghn": ["wait", "markNext:git"],
    "ghN": ["wait", "markPrev:git"],
    "ghs": "gitStage",
    "ghu": "gitUnstage"
  },
  "explorer.git.showIgnored": false,
  "explorer.icon.enableNerdfont": true,
	"diagnostic.virtualTextCurrentLineOnly": false,
	"diagnostic.virtualText": true
}

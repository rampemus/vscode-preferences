{
  "git": {
    "addedSign.hlGroup": "GitGutterAdd",
    "changedSign.hlGroup": "GitGutterChange",
    "modifiedSign.hlGroup": "GitGutterChange",
    "removedSign.hlGroup": "GitGutterDelete",
    "topRemovedSign.hlGroup": "GitGutterDelete",
    "changeRemovedSign.hlGroup": "GitGutterChangeDelete"
  },
  "snippets.textmateSnippetsRoots": [
    "~/Library/Application Support/Code/User/snippets"
  ],
  "snippets.loadFromExtensions": false,
  "snippets.priority": 0,
  "explorer.keyMappings.global": {
    "o": [
      "wait",
      "expandable?",
      ["wait", "expanded?", "collapse", "expand"],
      "open"
    ],
    "p": ["open", "normal:-"],
    "-": "collapse",
    "E": "open:split",
    "s": "open:vsplit",
    "d": "delete",
    ".": "toggleHidden",
    "l": ["adjustSize:5", "refresh"],
    "h": ["adjustSize:-5", "refresh"],
    "ghn": ["wait", "markNext:git"],
    "ghN": ["wait", "markPrev:git"],
    "ghs": "gitStage",
    "ghu": "gitUnstage"
  },
  "explorer.git.showIgnored": false,
  "explorer.icon.enableNerdfont": true,
  "explorer.floating.border.title": "Explorer",
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.virtualText": true,
  "suggest.minTriggerInputLength": 4,
  "suggest.autoTrigger": "always",
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "Lua.diagnostics.globals": ["vim"]
}

{
    "atomKeymap.promptV3Features": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "git.confirmSync": false,
    "git.autofetch": true,
    "explorer.confirmDelete": false,
    "window.zoomLevel": 0,
    "workbench.editor.tabCloseButton": "left",
    "editor.scrollBeyondLastLine": false,
    "terminal.integrated.rendererType": "dom",
    "vim.statusBarColorControl": false,
    "editor.minimap.enabled": false,
    "vim.insertModeKeyBindings": [
        {
            "before":["k","k","k"],
            "after": ["<Esc>","l"]
        },
        {
            "before": ["j","j"],
            "after": ["<Esc>","l"]
        },
        {
            "before": ["j","k"],
            "after": ["<Esc>","l"] 
        },
        {
            "before": ["k","j"],
            "after": ["<Esc>","l"]
        },
        {
            "before": ["<tab>"],
            "after": ["editor.action.triggerSuggest"]
        }
    ],
    "vim.normalModeKeyBindings": [
        {
            "before": ["<tab>"],
            "commands": ["editor.action.indentLines"]
        },
        {
            "before": ["<shift+tab>"],
            "commands": ["editor.action.outdentLines"]
        },
        {
            "before": ["รถ"],
            "after": ["/"]
        },
        {
            "before": ["ร"],
            "after": ["?"]
        },
        {
           "before": ["+"],
            "after": ["$"]
        },
        {
          "before": ["K"], // for especially tags, json objects and logical
          "after": ["$","?","[","^","=",":","\\","&","\\","|","\\","?","]"," ","<enter>","l","x","i","<enter>","<Esc>","k"]
        },
        {
            "before": ["\\"],
            "commands": ["editor.action.commentLine"]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": ["<tab>"],
            "commands": ["editor.action.indentLines"]
        },
        {
            "before": ["<shift+tab>"],
            "commands": ["editor.action.outdentLines"]
        },
        {
            "before": ["\\"],
            "commands": ["editor.action.commentLine"]
        }
    ],
    "vim.camelCaseMotion.enable": true,
    "workbench.colorCustomizations": {
        "editor.selectionHighlightBorder": "#545454"
    },
    "workbench.colorTheme": "One Dark Pro",
    "editor.highlightActiveIndentGuide": false,
    "git.enableSmartCommit": true,
    "todo-tree.highlights.enabled": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "explorer.confirmDragAndDrop": false,
    "task.slowProviderWarning": [ "typescript" ],
    "task.autoDetect": "off",
    "vim.highlightedyank.enable": true,
    "beautify.config": {
        "indent_size": 2,
        "indent_char": " ",
        "css": {
            "indent_size": 4
        },
        "json": {
            "indent_size": 2
        }
    },
    "editor.occurrencesHighlight": false,
    "workbench.editor.enablePreview": false, 
    "editor.lineNumbers": "relative",
    "oneDarkPro.vivid": true,
    "editor.gotoLocation.multipleDefinitions": "goto",
    "editor.gotoLocation.multipleImplementations": "goto",
    "workbench.list.automaticKeyboardNavigation": false,
    "search.exclude": {
        "**/build/**": true,
        "**/dist/**": true,
        "**/package-lonk.json": true
    },
    "editor.autoClosingOvertype": "always",
    "editor.autoClosingQuotes": "always",
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "editor.tokenColorCustomizations": {
        "comments": "#a14646"
    },
    "typescript.format.semicolons": "remove",
    "typescript.preferences.quoteStyle": "single",
    "javascript.preferences.quoteStyle": "single",
    "javascript.format.semicolons": "remove",
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "gitlens.codeLens.enabled": false,
    "gitlens.codeLens.authors.enabled": false,
    "gitlens.blame.format": "${author|4} ${message|30?} ${agoOrDate|14-}",
    "gitlens.blame.highlight.enabled": false,
    "gitlens.blame.highlight.locations": [
        "gutter",
        "overview",
        "line"
    ],
    "gitlens.currentLine.enabled": false,
    "vscode-neovim.neovimInitVimPaths.darwin": "/Users/rampemus/Library/Application Support/Code/User/config/init.vim",
    "vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",
    "editor.cursorSurroundingLines": 9,
    "editor.matchBrackets": false,
    "subtleBrackets.style": {
        "borderStyle": "solid",
        "borderColor": "#666",
        "borderRadius": "6px"
    }
}